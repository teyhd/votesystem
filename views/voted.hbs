<p class="center">Голоса</p>
<div class="row">
    <div class="col s12">
        <table border="1" class="responsive-table centered striped highlight ">
                <thead>
                <tr>
                    <th>Имя</th>
                    <th>Училище</th>
                    <th>Фильм</th>
                    <th>Номинация</th>
                    <th>Результат</th>
                </tr>
                </thead>
                {{#OK}}{{/OK}}
                <tbody>
                    {{#each content}}
                    <tr>
                    <td>{{this.name}}</td>
                    <td>{{this.pku}}</td>
                    <td>{{this.film}}</td>
                    <td>{{this.type}}</td>
                    <td>{{this.result}}</td>
                    </tr>    
                    {{/each}}
                </tbody>
        </table>

    </div>
